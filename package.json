{
  "name": "flip",
  "version": "1.0.0",
  "description": "Swedish marketplace flipping platform",
  "scripts": {
    "dev": "npm run dev:preflight && next dev",
    "build": "next build",
    "build:full": "npm run type-check && npm run lint && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:strict:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:strict:fix && npm run format",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:preflight": "node scripts/preflight-check.js",
    "task:start": "node scripts/task-starter.js",
    "task:complete": "node scripts/task-completer.js",
    "validate:phase-requirements": "node scripts/validate-phase.js",
    "validate:rate-limits": "node scripts/check-rate-limits.js",
    "test:price-db": "echo \"Price database tests would run here\"",
    "test:security": "echo \"Security tests would run here\"",
    "test:scrapers": "echo \"Scraper tests would run here\"",
    "db:migrate": "tsx lib/database/scripts/migrate.ts",
    "db:migrate:status": "tsx lib/database/scripts/status.ts",
    "db:seed": "tsx lib/database/scripts/seed.ts",
    "db:seed:clear": "tsx lib/database/scripts/clear-seeds.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:seed:clear && npm run db:migrate && npm run db:seed",
    "db:validate": "tsx lib/database/scripts/validate.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "discord.js": "^14.22.1",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "puppeteer": "^24.17.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.16.17",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.0",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "*.{json,md,css,scss,yaml,yml}": [
      "prettier --write"
    ],
    "*.sql": [
      "prettier --write --parser sql"
    ]
  }
}

# Platform Credentials (Encrypted storage)
PLATFORM_CREDENTIALS_ENCRYPTION_KEY=your_encryption_key

# Rate Limiting
REDIS_URL=your_redis_connection_string

# Monitoring
SENTRY_DSN=your_sentry_dsn
POSTHOG_KEY=your_posthog_key